Private Sub cmdInit_Click()
Adodc1.Recordset.MoveFirst
  Do While Not Adodc1.Recordset.EOF
    With Adodc2.Recordset
      .AddNew
      !AcademicID = AcademicID
      !ClassRecordID = ClassRecordID
      !Studid = Adodc1.Recordset.Fields("StudID").Value
      !Grade = 0
      .Update
      Adodc2.Refresh
    End With
    Adodc1.Recordset.MoveNext
  Loop
  Adodc2.Refresh
  
End Sub

Private Sub Form_Load()
 Adodc1.RecordSource = "SELECT * FROM tblStudSection WHERE SectionID =" & SectionID
 Adodc1.Refresh
End Sub
